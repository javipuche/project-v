{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"],
      "@components/*": ["./src/components"],
      "@layouts/*": ["./src/layouts"],
      "@pages/*": ["./src/pages"],
      "@store/*": ["./src/scss/themes/store"],
      "@mixins/*": ["./src/mixins"]
    }
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
